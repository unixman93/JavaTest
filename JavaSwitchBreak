public class Pasport {


    public static void main(String[] args) {


        switch(2){

            case 1: System.out.println(1);

            break;

            case 2: System.out.println(2);

            break;

            case 3: System.out.println(3);

            break;

            default: System.out.println("DefaultCase");

            break;




        }



    }
}
