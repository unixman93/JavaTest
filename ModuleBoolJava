public class A {


    public static void main(String[] args) {




        int a = 3;
        a=a<<2;
        System.out.println(a);

        a=a>>2;
        System.out.println(a);

        int b=12;
        System.out.println(b>>3);



        int c = 27;//*29   ---- umnozhenie na 29, realizacija na shiftah

        c=((c<<4)+(c<<3)+(c<<2)+(c));

        System.out.println(c);


    }


}
